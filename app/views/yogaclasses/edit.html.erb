
<h2> Edit Yogaclass </h2>

<form method="POST" action="/yogaclasses/<%=@yogaclass.id%>">
  <input type="hidden" name="_method" value="PATCH">
    <label for="yogaclass">Level</label>
  <input id = "yogaclass" type="text" name="yogaclass" value="<%= @yogaclass.yogaclass %>">
  <input type="text" name="instructor" value="<%= @yogaclass.instructor %>">
  <input type="text" name="description" value="<%= @yogaclass.description %>">
  <input type="text" name="date" value="<%= @yogaclass.date %>">
  <input type="submit" value="submit">
</form>
